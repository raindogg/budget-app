<div class="container" id="graph-page">
  <div class="row">
    <div class="col-xs-12">
      <div class="info text-center">
        <h1><%= @category.name %> month by month</h1>
        <h2>Spending goal: <%= number_to_currency(@goal) %></h2>
      </div>
      <div class="details">
      </div>
      <div class="chart-area">
        <svg class="chart">

        </svg>
      </div>
    </div>
  </div>
</div>
<div class="hidden">
  <ul class="values">
<% @values.each do |value| %>
  <li><%= value %></li>
<% end %>
  </ul>
  <ul class="months">
<% @months.each do |month| %>
    <li><%= month[:name] %> <%= month[:year] %> <%= month[:id] %></li>
<% end %>
  </ul>
  <p id="goal"><%= @goal %></p>
</div>